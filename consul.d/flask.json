{
  "service": {
    "name": "flask",
    "port": 5000,
    "connect": {
      "sidecar_service": {}
    },
    "check": {
      "id": "api",
      "name": "HTTP API on port 5000",
      "http": "http://flask:5000/healthcheck",
      "tls_skip_verify": true,
      "method": "GET",
      "header": {"Content-Type": ["application/json"]},
      "interval": "60s",
      "timeout": "1s"
    }
  }
}